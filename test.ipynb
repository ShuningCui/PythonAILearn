   "source": [
    "string=input().split()\n",
    "k=int(string[1])\n",
    "string1=string[0]\n",
    "out=[]\n",
    "for i in string1:\n",
    "    if i.isupper():\n",
    "        i=chr((ord(i)-65+k)%26+65)\n",
    "    if i.islower():\n",
    "        i=chr((ord(i)-97+k)%26+97)\n",
    "    out.append(i)\n",
    "print(''.join(out))"
   ]
  }
